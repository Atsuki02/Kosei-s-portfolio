---
import { ContactSection } from "../contact/ContactSection";
import Work from "../work/Work";


const {images} = Astro.props
---

	<main class="flex items-center flex-col h-full min-h-screen bg-transparent px-6 py-20 md:px-12 md:py-24">
	  <div class="w-full max-w-7xl space-y-24 md:space-y-32">
			<Work images={images} client:visible />
			<ContactSection client:visible />
		</div>
	</main>
